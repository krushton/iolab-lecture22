{
    "name": "Gif Cubby",
    "version": "1.3",
    "manifest_version": 2,
    "description": "Extension for saving gifs to Gif Cubby (gifcubby.com)",
    "icons": { "16": "icon-bitty.png",
                "48": "icon-small.png",
                "128": "icon-large.png"},
    "background": {
        "scripts": [ 
           "jquery.min.js",
           "gifs.js"
        ]
      },
    "permissions": [ 
        "contextMenus",
        "http://gifcubby/*"
    ]
}


